<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>pbi_dax_formatter</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AnalysisServices.NetCore.retail.amd64" Version="19.51.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <Using Include="Microsoft.AnalysisServices.Tabular" />
    <Using Include="Newtonsoft.Json" />
    <Using Include="System.Net.Http.Headers" />
    <Using Include="System.Text" />
    <Using Include="System.Security.Cryptography" />
  </ItemGroup>

</Project>
